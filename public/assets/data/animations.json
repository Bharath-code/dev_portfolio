{
	"entryAnimations": [
		{
			"selector": "header",
			"from": {
				"y": -40,
				"opacity": 0,
				"duration": 0.8,
				"ease": "power2.out"
			}
		},
		{
			"selector": "main section:first-of-type > div",
			"from": {
				"y": 30,
				"opacity": 0,
				"duration": 1,
				"ease": "power2.out",
				"stagger": 0.18,
				"delay": 0.1
			}
		}
	],
	"scrollAnimation": {
		"selector": "main section",
		"skipFirst": true,
		"from": {
			"scrollTrigger": {
				"start": "top 80%",
				"once": true
			},
			"y": 40,
			"opacity": 0,
			"duration": 0.9,
			"ease": "power2.out"
		}
	}
}
